    "./libs": {
      "import": "./dist/libs.es.js",
      "types": "./dist/libs.d.ts",
      "require": "./dist/libs.cjs.js"
    },
    "./hooks": {
      "import": "./dist/hooks.es.js",
      "types": "./dist/hooks.d.ts",
      "require": "./dist/hooks.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/node": "^24.9.1",
    "@vitest/coverage-v8": "^4.0.4",
    "jsdom": "^27.0.1",
    "terser": "^5.44.0",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.4"
  }
