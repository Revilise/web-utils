{
  "name": "@revilise/web-utils",
  "version": "1.0.2",
  "description": "Utility functions",
  "author": {
    "name": "Anastasia Mutnykh",
    "email": "mutnyh.ao@mail.ru"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/revilise/web-utils"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "main": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "vite build",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "types": "./dist/index.d.ts",
